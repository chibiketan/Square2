{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "system",
    //"module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": false,
    //"outDir": "../wwwroot/appScripts/",
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "moduleResolution": "node",
    "lib": [ "es2015", "dom" ],
    "declaration": false
  },
  "exclude": [
    "node_modules",
    "typings/index",
    "typings/index.d.ts"
  ],
  "compileOnSave": false
}